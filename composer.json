{
    "name": "technically/dependency-resolver",
    "description": "Dependency-resolver utility based on PSR-11 container to be used for auto-wiring.",
    "type": "library",
    "keywords": [
        "php",
        "psr-11",
        "dependency-resolver",
        "auto-wiring"
    ],
    "require": {
        "php": "^8.0",
        "psr/container": "^1.0|^2.0",
        "technically/null-container": "^1.0",
        "technically/callable-reflection": "^0.4.0"
    },
    "require-dev": {
        "peridot-php/peridot": "^1.19",
        "technically/array-container": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Voskoboinyk",
            "email": "ivan@voskoboinyk.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Technically\\DependencyResolver\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Technically\\DependencyResolver\\Specs\\": "specs"
        }
    }
}
